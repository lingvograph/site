{
  "name": "lingvograph-site",
  "description": "lingvograph site",
  "version": "1.0.0",
  "contributors": [
    "Imed Adel <aadelimed@gmail.com>",
    "Sergey Todyshev <stodyshev@gmail.com>"
  ],
  "dependencies": {
    "gatsby": "^2.21.9",
    "gatsby-image": "^2.4.0",
    "gatsby-plugin-google-analytics": "^2.3.0",
    "gatsby-plugin-manifest": "^2.4.1",
    "gatsby-plugin-netlify": "^2.3.0",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-sitemap": "^2.4.1",
    "gatsby-plugin-styled-components": "^3.3.0",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-sharp": "^2.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^1.2.5",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.3.2",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "babel-plugin-styled-components": "^1.10.7",
    "gh-pages": "2.0.1",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "PATH_PREFIX=/site/ gatsby build --prefix-paths",
    "rebuild": "npm run clean && npm run build",
    "dev": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "predeploy": "npm run rebuild",
    "deploy": "gh-pages --dist public --add --dotfiles"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern src/**/*.{js,jsx}"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
